{
    "testEnvironment": "node",
    "testMatch": [
        "**/__tests__/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
        "controllers/**/*.js",
        "middleware/**/*.js",
        "models/**/*.js",
        "!models/index.js"
    ],
    "transform": {},
    "setupFilesAfterEnv": [
        "<rootDir>/__tests__/setup.js"
    ],
    "testTimeout": 30000
}